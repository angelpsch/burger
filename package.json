  "license": "ISC",
  "bugs": {
    "url": "https://github.com/angelpsch/burger/issues"
  },
  "homepage": "https://github.com/angelpsch/burger#readme",
  "dependencies": {
    "express": "^4.17.1",
    "express-handlebars": "^4.0.4",
    "handlebars": "^4.7.6",
    "mysql": "^2.18.1",
    "sequelize": "^6.3.4"
  }
}
